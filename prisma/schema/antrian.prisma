model AntrianPasien {
    id                      Int                     @id @default(autoincrement())
    tanggal                 DateTime
    status                  String
    episodePendaftaranId    Int?                    @unique
    episodePendaftaran      EpisodePendaftaran?     @relation(fields: [episodePendaftaranId], references: [id])
    jumlahPanggil           Int
    nomor                   String
    idFasyankes             String
    createdAt               DateTime                @default(now())
    updatedAt               DateTime                @updatedAt
}


model AntrianAdmisi {
    id                      Int                     @id @default(autoincrement())
    nomor                   String
    tanggal                 DateTime
    status                  String
    jumlahPanggil           Int
    idFasyankes             String
    createdAt               DateTime                @default(now())
    updatedAt               DateTime                @updatedAt
}