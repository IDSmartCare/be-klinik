model AntrianPasien {
    id                      Int             @id @default(autoincrement())
    tanggal                 DateTime
    status                  String
    pendaftaranId           Int?            @unique
    pendaftaran             Pendaftaran?    @relation(fields: [pendaftaranId], references: [id])
    jumlahPanggil           Int
    nomor                   String
    idFasyankes             String
    createdAt               DateTime @default(now())
    updatedAt               DateTime @updatedAt

}


model AntrianAdmisi {
    id                      Int         @id @default(autoincrement())
    nomor                   String
    tanggal                 DateTime
    status                  String
    jumlahPanggil           Int
    idFasyankes             String
    createdAt               DateTime @default(now())
    updatedAt               DateTime @updatedAt
}