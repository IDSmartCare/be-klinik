model DoctorAvailableDays {
  id        Int      @id @default(autoincrement())
  doctorId  Int
  sun       String?
  mon       String?
  tue       String?
  wen       String?
  thu       String?
  fri       String?
  sat       String?
  deletedAt DateTime? @map("deleted_at")
  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @updatedAt @map("updated_at")

}
